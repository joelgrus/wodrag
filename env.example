# Copy this file to .env and fill in your values

# Language Model Configuration
# Default model (OpenRouter Gemini Flash Lite - cost effective)
# WODRAG_LM_MODEL=openrouter/google/gemini-2.5-flash-lite

# API Keys (required based on model choice)
# Required if using openrouter/* models (default)
OPENROUTER_API_KEY=your-openrouter-api-key
# Required if using openai/* models  
OPENAI_API_KEY=your-openai-api-key

# Database (defaults shown)
POSTGRES_PASSWORD=your-secure-password-here
DATABASE_URL=postgresql://postgres:your-secure-password-here@localhost:5432/wodrag

# Production settings (override defaults)
# WODRAG_API_CORS_ORIGINS='["https://your-domain.com"]'